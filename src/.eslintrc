{
  "root": true,
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "es6": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "array-bracket-spacing": 1,
    "arrow-parens": 1,
    "arrow-spacing": 1,
    "brace-style": 1,
    "camelcase": 1,
    "comma-dangle": ["error", {"arrays": "always-multiline", "objects": "always-multiline"}],
    "comma-style": 1,
    "dot-notation": 1,
    "eol-last": 1,
    "eqeqeq": 1,
    "func-style": [1, "expression"],
    "id-length": "error",
    "import/first": 1,
    "import/no-mutable-exports": 1,
    "import/no-webpack-loader-syntax": 1,
    "import/prefer-default-export": 1,
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "jsx-quotes": 1,
    "key-spacing": 1,
    "keyword-spacing": 1,
    "max-len": ["error", { "code": 100, "ignoreComments": true }],
    "new-cap": 1,
    "newline-per-chained-call": 1,
    "no-alert": 0,
    "no-array-constructor": 1,
    "no-case-declarations": 1,
    "no-confusing-arrow": 1,
    "no-console": 1,
    "no-const-assign": 1,
    "no-debugger": 1,
    "no-dupe-class-members": 1,
    "no-duplicate-imports": 1,
    "no-iterator": 1,
    "no-lone-blocks": 0,
    "no-loop-func": 1,
    "no-multi-spaces": 1,
    "no-nested-ternary": 1,
    "no-new-object": 1,
    "no-new-func": 1,
    "no-param-reassign": 2,
    "no-plusplus": 1,
    "no-restricted-syntax": ["error", "ForStatement", "ForInStatement", "ForOfStatement"],
    "no-tabs": 1,
    "no-trailing-spaces": 0,
    "no-undef": 1,
    "no-underscore-dangle": 0,
    "no-unneeded-ternary": 1,
    "no-useless-constructor": 1,
    "no-useless-escape": 1,
    "no-var": 1,
    "no-whitespace-before-property": 1,
    "object-curly-spacing": [1, "always"],
    "object-shorthand": 1,
    "one-var": [1, "never"],
    "padded-blocks": [1, "never"],
    "prefer-arrow-callback": 1,
    "prefer-const": 1,
    "prefer-rest-params": 1,
    "prefer-spread": 1,
    "quotes": 0,
    "quote-props": [1, "as-needed"],
    "semi": [1, "always"],
    "space-before-blocks": 1,
    "space-before-function-paren": ["error", { "anonymous": "always", "named": "ignore"}],
    "space-in-parens": 1,
    "space-infix-ops": 1,
    "spaced-comment": 1,
    "wrap-iife": [1, "outside"],
  },
  "globals": {
  }
}
